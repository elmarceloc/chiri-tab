{
    "author": "elmarceloc",
    "name": "Chiri Tab",
    "version": "0.1",
    "manifest_version": 3,
    "minimum_chrome_version": "88",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "description": "A chrome extension that shows a random image of my cat in a new tab.",
    "permissions" : ["management"],
    "chrome_url_overrides" : {
        "newtab": "src/index.html"
    }
}
